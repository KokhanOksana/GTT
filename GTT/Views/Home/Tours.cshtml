@model IEnumerable<GTT.Models.TourView>
@{
    ViewBag.Title = "Tours";
}

<h2>Tours</h2>
@foreach (var t in Model)
{
    @t.ID<br />
    @t.Name<br />
    @t.FoodType<br />
    @t.TourDate.First()<br />
    @t.PriceUSD<br />
    @t.Description<br />
    <img src="@t.Picture.First()"><br />
}

